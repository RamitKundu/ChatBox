app.controller('UserController',['UserService','$http','$scope','$location',function(UserService,$http,$scope,$location){
	var self=this;
	self.user={};
	
	this.addUser=function(){
		UserService.addUser()
		.then(
				function(response)
				{
					console.log(response.data);
					$location.path('/user');
				},
				function(error){
					console.log(error);})} 
	
	
	
}
	

	
	
	])      